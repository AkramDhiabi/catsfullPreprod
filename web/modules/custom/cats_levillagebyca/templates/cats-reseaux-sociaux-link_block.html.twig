		<button class="ca-header-webradio-button ca-fonts-montserrat-medium" onclick="window.open('{{ webradiourlen }}', '_blank')" type="button">{{ webradiotitlebuttonen }}&nbsp;&nbsp; 
			<img alt="micro-web-radio" class="svg" data-entity-type="file" data-entity-uuid="2fcb0613-092d-42e7-a58b-0810ebf54a5b" src="/sites/default/files{{ iconurlen }}" />
		</button>

		<div class="ca-fonts-montserrat-medium btn-primary-bis header-link-social-network">{{ rstitleen }}</div>
		<a class="social-links-inter-margin" href="{{ yurlen }}" target="_blank" title="{{ ytitleen }}">
			<img alt="youtube-little" data-entity-type="file" data-entity-uuid="f6c722a4-d576-43bf-bb80-c86ea43c1607" src="/sites/default/files{{ yiconpathen }}" />
		</a>
		<a class="social-links-inter-margin" href="{{ turlen }}" target="_blank" title="{{ ttitleen }}">
			<img alt="twitter-little" data-entity-type="file" data-entity-uuid="f34ba27c-9027-4a73-89a2-6ea5ba37e5c4" src="/sites/default/files{{ ticonpathen }}" />
		</a>
		<a class="social-links-extra-margin" href="{{ lurlen }}" target="_blank" title="{{ ltitleen }}">
			<img alt="linkedin-little" data-entity-type="file" data-entity-uuid="7e6ce9b1-802f-465e-b87e-9e45a6d55d49" src="/sites/default/files{{ liconpathen }}" />
		</a>
		<div class="ca-fonts-montserrat-medium btn-primary-bis header-link-social-network" id="header-link-social-network-lang">{{ languageTitleen }}</div>
	</div>
{% endif %}